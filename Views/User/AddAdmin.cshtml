@model EcommerceOfficial.RequestModel.RegisterUserRequestModel;


<p style="color: red; font-weight: bold;">Add aadmin</p>
<form method="Post" enctype="multipart/form-data">
    <div class="col-md-6" style = "margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="FirstName" class= "form-label">Firstname</label>
        <input asp-for = "FirstName" type="text" class="form-control">
    </div>
    <div class="col-md-6" style="margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="LastName" class="form-label">Lastname</label>
        <input asp-for="LastName" type="text" class="form-control">
    </div>
    <div class="col-md-6" style="margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="Email" class="form-label">Email</label>
        <input asp-for="Email" type="text" class="form-control">
    </div>
    <div class="col-md-6" style="margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="Password" class="form-label">Password</label>
        <input asp-for="Password" type="password" class="form-control">
    </div>
    <div class="col-md-6" style="margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="ConfirmPassword" class="form-label">ConfirmPassword</label>
        <input asp-for="ConfirmPassword" type="password" class="form-control">
    </div>
    <div class="col-md-6" style="margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="PhoneNumber" class="form-label">PhoneNumber</label>
        <input asp-for="PhoneNumber" type="text" class="form-control">
    </div>
    <div class="col-md-6" style="margin-top:40px; font-size: larger; font -weight: bold;colour: darkcyan">
        <label asp-for="ProfilePicture" class="form-label">ProfilePicture</label>
        <input asp-for="ProfilePicture" type="file" class="form-control">
    </div>
    <button type="submit">Create</button>
</form>